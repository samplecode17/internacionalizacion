msgid "Concatenar dos cadenas"
msgstr "Concatena dues cadenes"

#: main.py:17
msgid "Cambiar a mayúsculas"
msgstr "Canvia a majúscules"

#: main.py:18
msgid "Cambiar a minúsculas"
msgstr "Canvia a minúscules"

#: main.py:19
msgid "Contar caracteres"
msgstr "Compta caràcters"

